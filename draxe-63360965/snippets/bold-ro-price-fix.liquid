{% assign ro_cart_total = 0 %}
{% for item in cart.items %}
{% if item.properties.discounted_price != blank %}
{% assign line_price = item.properties.discounted_price | replace: '.', 'v' | handleize | replace: 'v', '.' | times: 100 | money_without_currency | times: 100 | times: item.quantity %}
{% assign ro_cart_total = ro_cart_total | plus: line_price %}
{% else %}
{% assign ro_cart_total = ro_cart_total | plus: item.line_price %}
{% endif %}
{% endfor %}